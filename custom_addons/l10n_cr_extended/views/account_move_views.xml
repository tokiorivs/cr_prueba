<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_move_form_inherit_l10n_cr_custom" model="ir.ui.view">
        <field name="name">account.move.form.inherit.l10n.cr.custom</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <group>
                    <field  name="electronic_number"/>
                    <field  name="electronic_consecutive"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
